function maps(x){
  let array = []
  for(let i = 0; i < x.length; i++){
    if(x[i] === x[i]){
    array.push(x[i] + x[i])
    }
    
  }
    return array 
}


/*

PREP

Parameter: x an array of integers

Return: a new array with each value doubled

Example:
[2,4,6] --> [4,8,12]

Psuedocode:
1. Given an array of integers
2. Return a new array with each value doubled
create a variable for the new array
IF/ELSE conditional
itterate through the array FOR LOOP
array.push to create a new array
*/